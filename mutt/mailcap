#text/html; ${HOME}/.mutt/view_attachment.sh "exo-open --launch WebBrowser" %s; sleep 1; test=test -n "$DISPLAY";
text/html; ${HOME}/.mutt/view_attachment.sh x-www-browser %s; sleep 1; test=test -n "$DISPLAY";
text/html; lynx -dump %s; copiousoutput;
text/plain; vim %s;
application/pdf; ${HOME}/.mutt/view_attachment.sh xpdf %s;
image/jpeg; ${HOME}/.mutt/view_attachment.sh geeqie %s;
image/tiff; ${HOME}/.mutt/view_attachment.sh geeqie %s;
image/png; ${HOME}/.mutt/view_attachment.sh geeqie %s;
application/vnd.ms-excel; ${HOME}/.mutt/view_attachment.sh soffice %s;
application/vnd.ms-word; ${HOME}/.mutt/view_attachment.sh soffice %s;
application/msword; ${HOME}/.mutt/view_attachment.sh soffice %s;

text/calendar; mutt-ics; copiousoutput
application/ics; mutt-ics; copiousoutput
